check if two strings are permutation of each other


import collections
class Solution():
    def unique(str1, str2):
        if not str1 and not str2:
            return True
        elif not str1 or not str2:
            return False
        # else:
        #     return False    
       

        count = collections.Counter(str1)
        count1 = collections.Counter(str2)

        if count == count1:
            return True
        else:
            return False    

            

             

    str1=""
    str2=""
    print(unique(str1, str2))              


